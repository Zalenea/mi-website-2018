{{ define "response" }}
[	 
    {{ range $index, $element := .Data.Pages }}
    {{ if $index }}, {{ end }}
    {
    "title":"{{$element.Title}}",
    "termin":"{{$element.Params.termin}}",
    "date":"{{$element.Params.date}}",
    "content": {{$element.RawContent  | jsonify }}
    }
    
    {{ end }}
]
{{ end }}